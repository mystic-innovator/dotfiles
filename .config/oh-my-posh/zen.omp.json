{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "foreground": "#fafafa",
          "type": "os",
          "style": "plain"
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "#2EEFBF",
          "powerline_symbol": "\ue0b0",
          "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ if ne .Env.POSH_SESSION_DEFAULT_USER .UserName }}{{ .UserName }}@{{ end }}{{ .HostName }} ",
          "properties": {
            "display_host": true,
            "display_user": true
          }
        },
        {
          "template": " \uf0e7 ",
          "foreground": "#ffff66",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\ue5fe ",
            "folder_separator_icon": "\ue0bf ",
            "home_icon": "\uf015 ",
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} ",
          "foreground": "#fafafa",
          "type": "path",
          "style": "powerline"
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#D3D3D3",
          "template": "{{ if .UpstreamIcon }}{{ .UpstreamIcon }} {{ end }}{{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf0e7 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf00c {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf0c7 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0 }} \uf1bb {{ .WorktreeCount }}{{ end }} ",
          "foreground_templates": [
            "{{ if gt .StashCount 0 }}#FF69B4{{ else if gt .WorktreeCount 0 }}#FFD700{{ else if or (.Working.Changed) (.Staging.Changed) }}{{ if .Staging.Changed }}#00FF00{{ else }}#FFEB3B{{ end }}{{ else if gt .Ahead 0 }}#B388FF{{ else if gt .Behind 0 }}#FF5555{{ else }}#00FFFF{{ end }}"
          ],
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_stash_count": true,
            "fetch_worktree_count": true,
            "branch_max_length": 25,
            "cache_duration": "5m",
            "mapped_branches": {
              "feat/*": "\uf135 ",
              "fix/*": "\uf0ad ",
              "hotfix/*": "\uf06d "
            },
            "status_formats": {
              "working": "{{ if .Changed }}{{ .Added }}+{{ .Modified }}~{{ .Deleted }}-{{ .Untracked }}?{{ end }}",
              "staging": "{{ if .Changed }}{{ .Added }}+{{ .Modified }}~{{ .Deleted }}-{{ end }}"
            }
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "upgrade",
          "style": "plain",
          "foreground": "#111111",
          "background": "#FFD664",
          "properties": {
            "cache_duration": "168h"
          }
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_package_manager": true,
            "home_enabled": true
          },
          "foreground": "#A3BE8C",
          "type": "node",
          "style": "plain"
        },
        {
        "template": " \ue235 {{ if .Venv }}{{ .Venv }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#96E072",
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 250
          },
          "template": " \u231b {{ .FormattedMs }} ",
          "foreground": "#FFCE5C",
          "type": "executiontime",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "~#@‚ùØ",
          "foreground": "#7a7a7a",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
